{
    "name": "carestad/google-crawler",
    "description": "A simple Crawler for getting Google results",
    "keywords": ["google", "crawler", "search"],
    "type": "library",
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/dom-crawler": "^6.3.4",
        "php-ds/php-ds": "^1.4.1",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.13"
    },
    "suggest": {
        "ext-ds": "Allow IDE autocomplete for editing the component"
    },
    "scripts": {
        "phpunit": "phpunit --coverage-clover=coverage.clover"
    },
    "autoload": {
        "psr-4": {
            "CViniciusSDias\\GoogleCrawler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CViniciusSDias\\GoogleCrawler\\Tests\\": "tests/"
        }
    },
    "license": "GPL-3.0"
}
